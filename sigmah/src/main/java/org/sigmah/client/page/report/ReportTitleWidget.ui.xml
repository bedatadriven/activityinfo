<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder">
	<ui:style type="org.sigmah.client.page.report.ReportTitleWidget.MyStyle">
		.reportTitle {
			color: #A66F4A;
			font-size: 16px;
			font-weight: bold;
			cursor: text;
		}
		
		.reportTitleEditor {
			font-size: 16px;
			font-weight: bold;
		}
		
		.changeTitleText {
			color: #A66F4A;
			font-size: 10px;
			cursor: pointer;
		}
	</ui:style>
	<div>
		<span class="{style.reportTitle}" ui:field="titleSpan" />
		<span class="{style.changeTitleText}">[change]</span>
	</div>
</ui:UiBinder>